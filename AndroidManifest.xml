<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.biubiu.biubiu"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <application
         android:name="com.android.biubiu.myapplication.BiubiuApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:largeHeap="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.android.biubiu.activity.WelcomeActivity"
            android:label="@string/app_name" >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.biubiu.activity.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.android.biubiu.activity.GuildActivity"
            android:label="@string/title_activity_biu_biu_send" >
        </activity>
         <activity
            android:name="com.android.biubiu.activity.LoginOrRegisterActivity"
            android:label="@string/title_activity_biu_biu_send" >
        </activity>
        <activity
            android:name="com.android.biubiu.activity.LoginActivity"
            android:label="@string/title_activity_biu_biu_send" >
        </activity>
        <activity
            android:name="com.android.biubiu.activity.RegisterActivity"
            android:label="@string/title_activity_biu_biu_send" >
        </activity>
        <activity
            android:name="com.android.biubiu.biu.activity.BiuBiuSendActivity"
            android:label="@string/title_activity_biu_biu_send" >
        </activity>
    </application>

</manifest>
